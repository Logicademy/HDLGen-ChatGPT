<head>
<?xml version="1.0" encoding="UTF-8"?>
<wave_config>
   <wave_state>
   </wave_state>
   <db_ref_list>
      <db_ref path="[componentName]_behav.wdb" id="1">
         <top_modules>
            <top_module name="[componentName]_TB" />
         </top_modules>
      </db_ref>
   </db_ref_list>

   <zoom_setting>
      <ZoomStartTime time="350ns"></ZoomStartTime>
      <ZoomEndTime time="1100ns"></ZoomEndTime>
      <Cursor1Time time="1100ns"></Cursor1Time>
   </zoom_setting>

   <column_width_setting>
      <NameColumnWidth column_width="250"></NameColumnWidth>
      <ValueColumnWidth column_width="70"></ValueColumnWidth>
   </column_width_setting>

   <WVObjectSize size="4" />
   <wvobject fp_name="/[componentName]_TB/testNo" type="other">
      <obj_property name="ElementShortName">testNo</obj_property>
      <obj_property name="ObjectShortName">testNo</obj_property>
   </wvobject>

   <wvobject fp_name="/[componentName]_TB/endOfSim" type="other">
      <obj_property name="ElementShortName">endOfSim</obj_property>
      <obj_property name="ObjectShortName">endOfSim</obj_property>
   </wvobject>
</head>
<UUT>
    <wvobject fp_name="UUTGroup" type="group">
      <obj_property name="label">UUT</obj_property>
      <obj_property name="DisplayName">label</obj_property>
      <wvobject fp_name="group30" type="group">
         <obj_property name="label">entity</obj_property>
         <obj_property name="DisplayName">label</obj_property>
         <obj_property name="isExpanded"></obj_property>
</UUT>
<UUTEnt>
    <wvobject fp_name="/[componentName]_TB/UUT/[signal]" type="[type]">
        <obj_property name="ElementShortName">[signal][size]</obj_property>
        <obj_property name="ObjectShortName">[signal][size]</obj_property>
    </wvobject>
</UUTEnt>
<UUTInt>
    </wvobject>
    <wvobject fp_name="UUTInternalSignalGroup" type="group">
        <obj_property name="label">internal</obj_property>
        <obj_property name="DisplayName">label</obj_property>
    <obj_property name="isExpanded"></obj_property>
</UUTInt>
<UUTInternal>
    <wvobject fp_name="/[componentName]_TB/UUT/[signal]" type="[type]">
         <obj_property name="ElementShortName">[signal][size]</obj_property>
         <obj_property name="ObjectShortName">[signal][size]</obj_property>
      </wvobject>
</UUTInternal>
<end>
  </wvobject>
</wave_config>
</end>